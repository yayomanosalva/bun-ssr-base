{
  "name": "bun-ssr-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run build:css:watch\" \"bun run server:dev\"",
    "build:css": "npx tailwindcss -i ./styles/globals.css -o ./dist/styles.css",
    "build:css:watch": "npx tailwindcss -i ./styles/globals.css -o ./dist/styles.css --watch",
    "build:client:dev": "bun build ./setup/client/entry-client.tsx --outdir ./dist --target=browser --format=esm --sourcemap=inline --entry-naming [name]-[hash].[ext]",
    "server:dev": "NODE_ENV=development bun --hot run ./setup/index.tsx",
    "predev": "bun run ./setup/scripts/ensure-css.js && bun run build:client:dev",
    "clean": "rm -rf dist ~/.bun/install/cache node_modules bun.lock",
    "postcss:watch": "bunx postcss ./styles/globals.css -o ./dist/styles.css --watch",
    "install:deps": "bun run ./setup/scripts/install-deps.ts",
    "install:all": "bun install && bun run ./setup/scripts/install-deps.ts",
    "setup": "bun run install:all"
  },
  "dependencies": {
    "@elysiajs/static": "^1.3.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@tanstack/react-router": "^1.131.31",
    "@tanstack/react-router-devtools": "^1.131.31",
    "autoprefixer": "^10.4.21",
    "bun-plugin-tailwind": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "elysia": "^1.3.21",
    "lucide-react": "^0.542.0",
    "postcss": "^8.5.6",
    "react": "^19",
    "react-dom": "^19",
    "react-helmet-async": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.34.0",
    "@rsbuild/core": "^1.5.3",
    "@rsbuild/plugin-react": "^1.4.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@tanstack/router-cli": "^1.131.30",
    "@tanstack/router-plugin": "^1.131.31",
    "@types/bun": "^1.2.21",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "bun-types": "^1.2.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.3",
    "globals": "^16.3.0",
    "playwright": "^1.55.0",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "tailwindcss": "3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  }
}